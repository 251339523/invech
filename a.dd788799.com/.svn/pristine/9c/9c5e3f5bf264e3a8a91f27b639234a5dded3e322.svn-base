
.common-footer-wrapper{
	width: 100%;
	height: 246px;
	background: #fff;
	border-top: 1px solid #f2f2f2;
}
.common-footer{
	width: 1000px;
	height: 100%;
	margin: 0 auto;
}
.common-choose{
	height: 91px;
	border-bottom: 1px solid #dcdcdc;
}
.common-choose a{
	display: block;
	float: left;
	width: 25%;
}
.common-choose a img{
	display: block;
	float: left;
	width: 47px;
	height: 47px;
	margin-top: 26px;
	margin-left: 40px;
}
.common-choose a b{
	display: block;
	float: left;
	font-size: 16px;
	color: #555555;
	margin-left: 10px;
	margin-top: 30px;
}
.common-choose a em{
	display: block;
	font-style: normal;
	font-size: 12px;
	margin-top: 8px;
}
.common-four-wrapper{
	width: 100%;
	height: 152px;
}
.common-time-wrapper{
	width: 166px;
	margin-left: 42px;
	height: 100%;
	float: left;
	overflow: hidden;
}
.common-time{
	clear: both;
	height: 20px;
	margin-top: 46px;
}
.common-time-top{
	margin-top: 25px;
}
.common-time b{
	text-align: left;
	font-size: 14px;
	color: #555555;
	font-weight: bold;
	display: block;
	float: left;
}
.common-time span{
	text-align: center;
	font-size: 14px;
	color: #555555;
	display: block;
	float: left;
	margin-left: 10px;
}
.common-time time{
	font-size: 14px;
	color: #555555;
	text-align: right;
	display: block;
	float: right;
}
.common-time-line{
	width: 100%;
	height: 6px;
	border-radius: 8px;
	background: #e2e2e2;
	clear: both;
}
.common-time-line p{
	display: block;
	height: 100%;
	border-radius: 8px;
	width: 25%;
	background: #00ff30;
}
.common-time-line-red p{
	width: 70%;
	background:#e40000 ;
}
.common-bank-wrapper{
	width: 222px;
	height: 102px;
	margin-left: 70px;
	float: left;
	padding-top: 50px;
}
.common-bank-wrapper a{
	display:block;
	width: 50%;
	height: 34px;
	float: left;
	margin-bottom: 10px;
}
.common-tip-wrapper{
	width: 165px;
	height: 100%;
	margin-left: 70px;
	float: left;
}
.common-tip-wrapper p{
	height: 24px;
	line-height: 24px;
}
.common-tip-wrapper p.firat-cont{
	margin-top: 20px;
}
.common-tip-wrapper p a{
	display: block;
	font-size: 14px;
	color: #555555;
}
.common-tip-wrapper p a.left{
	float: left;
	width: 50%;
}
.common-tip-wrapper p a.right{
	float: right;
    width: 40%;
    padding-left: 10%;
}
.common-liu-wrapper{
	width: 164px;
	height: 100%;
	margin-left: 70px;
	float: left;
}
.common-liu-wrapper a{
	display: inline-block;
	margin-left: 14px;
	margin-top: 50px;
}
