<script>
    $(function () {
        // 首页弹框登录按钮
        $('.loginbtn').click(function(){
            $(".bg").show();
            $(".login_alert").show();
        });
        $('.bg').click(function(){
            $(".bg").hide();
            $(".login_alert").hide();
        });
        $('.cha').click(function(){
            $(".bg").hide();
            $(".login_alert").hide();
        });
    });

    function goZst(url) {
        window.open('/ssc/'+ url +'.html?type=1');
    }

    function wjmm(url) {
        $(".spans").html("请联系在线客服");
        $(".spans").css("color","red");
    }

    function login() {
        var loginAccount = $("#registerLoginAccountAlert").val();
        var loginPassword = $("#registerLoginPasswordAlert").val();
        var yzm = $("#registerLoginYzmAlert").val();

        if (!loginAccount) {
            $(".yhm").html("请输入用户名3-15个英文、数字");
            $(".yhm").css("color","red");
            $("#mm").empty();
            $(".yzm").empty();
            return;
        }
        if (!loginPassword) {
            $("#mm").html("请输入密码");
            $("#mm").css("color","red");
            $("#mm").empty();
            $(".yzm").empty();
            return;
        }
        if (!yzm) {
            $("#yzm").html("请输入验证码");
            $("#yzm").css("color","red");
            $("#mm").empty();
            $(".yhm").empty();
            return;
        }

        ajaxRequest({
            url: "/member/ajaxLogin.json",
            data: {
                yzm: yzm,
                account: loginAccount,
                password: $.md5(loginPassword)
            },
            beforeSend: function () {
                showLoading();
            },
            success: function (json) {
                if (json.result == 1) {
                    $.cookie("uid", json.userId, {path: "/"});
                    $.cookie("token", json.token, {path: "/"});
                    window.location.href = "/main.html";
                } else {
                    refreshYzm(document.getElementById('registerYzmImg2'));
                    Tools.toast("登录失败：" + json.description);
                }
                hideLoading();
            }
        });
    }

    function showLoading() {
        layer.load(2, {
            shade: [0.1, '#000'] //0.1透明度的白色背景
        })
    }
    function hideLoading() {
        layer.closeAll();
    }
    function refreshYzm(obj) {
        var src = $(obj).attr("src");
        var params = getRequest(src);

        src = "/code/yzm?timestamp=" + (new Date()).getTime();
        $.each(params, function (index, value) {
            src += '&' + value.key + '=' + value.value;
        });
        console.log(src);
        $(obj).attr("src", src);
    }
</script>




