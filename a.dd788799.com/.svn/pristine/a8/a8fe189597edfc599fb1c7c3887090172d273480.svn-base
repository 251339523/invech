                $("#inputqq").blur(function(){
                    var qq = $("input[name='qq']").val();
                    if (!qq) {
                        $(".qq").find(".info").hide();
                        $('#yesqq').hide();
                        $('#noqq').show();
                        $(".qq").find(".error").html("请输入QQ").show();
