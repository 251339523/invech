<script type="text/javascript">
   var pusher = new Pusher('{$pusher_app_key}', {
     authEndpoint: "{:url('/broadcast/auth1')}",
     //authTransport: 'ajax',
     cluster: '{$pusher_cluster}',
     encrypted: true
   });

//   Pusher.logToConsole = true;
   var channel = pusher.subscribe('public-lottery.open.{$lottery_obj.code}');
   channel.bind('lottery.open', function(data) {
       data = data.code;

       var current_issue = $("#current_issue").html();
       if(current_issue == data.phase){//时间不同步提前开奖了
        kjTime = (Date.parse(new Date())/1000)-1;
       }

        var $dom = $('#kaijiang'), $kjHaoS, $feipan, hao;

        var type=game.type;
        if (parseInt(type) == 24) { //快8
            $kjHaoS = data.data.split('|');
            hao = $kjHaoS[0].split(',');
            $feipan = $kjHaoS[1];
        } else {
            hao = data.data.split(',');
        }

        //停止开奖转动
        if (moveno) clearInterval(moveno);
        var ctype = $('#showcodebox').attr('ctype');
        if (ctype == 'g1') {
            $('.kjhao li').each(function (i) {
                $(this).html(hao[i]);
            });
            if ($dom.find('.feipan')) $dom.find('.feipan').html("快乐飞盘：<em>" + $feipan + "</em>");
        } else if (ctype == 'k8') { //k8
            $('#showcodebox div').each(function (i) {
                $(this).text(hao[i]);
            });
        } else if (ctype == 'pk10') { //pk10
            $('#showcodebox div').each(function (i) {
                $(this).attr('class', 'gr_c gr_c' + hao[i]);
            });
        } else {

            console.log($('#showcodebox div'), hao);
            $('#showcodebox div').each(function (i) {
                $(this).attr('class', 'gr_s gr_s' + parseInt(hao[i]));
            });
        }

        //更新
        gameFreshOrdered();
        reloadMemberInfo();
   });

    var game={type:<?=json_encode($type)?>,	played:<?=json_encode($played)?>,groupId:<?=json_encode($groupId)?>}
    {if $user}
    var user="{$user.username}",aflag=<?=json_encode($user->admin==1)?>;
    {/if}
    var maxFandian = <?=$settings['fanDianMax']?>;
    var gfwf_pls_pair;

    $(function(){
        $("body").click(
            function toggle() {
                $("div#demo-set", window.top.document).removeClass("open");
                $(".mega-dropdown", window.top.document).removeClass("open");
                $(".dropdown", window.top.document).removeClass("open");
                $(".lskj").fadeOut(200);
            }
        );

        loadKjData();

        //console.log(gfwf_pls_pair)
        $('.slider-input').jRange({
            from: 0,
            to: maxFandian, // 最大返点
            step: 0.1,  // 每步
            format: '%s',
            width: $(".cl-1004").width(),   // 滚动条总长度
            theme: 'theme-green my-slide-theme',
            showLabels: false,
            showScale: false,
            snap: true,
            onstatechange: function () {
                // 返点比例
                var fandianBili = parseFloat($(".slider-input").val()).toFixed(1); // 当前滚动条移动的比例
                $("#fandian-bfb").data("value", fandianBili);
                $("#fandian-bfb").html(fandianBili + "%");    // 渲染界面中百分比部分

                $('#lt_sel_dyprize').attr('fan-dian',fandianBili);

                var convertBlMoneys = [];
                var pls = [];
                $.each(gfwf_pls_pair,function(key,pair){
                    var prop = parseFloat(pair.bonusProp);
                    var base = parseFloat(pair.bonusPropBase);
                    var convertBlMoney = (prop - base) / maxFandian;
                    //convertBlMoneys.push(fandian);

                    // 赔率 = 最大配率 - 返点比例 * 转换比例
                    var pl = (prop - fandianBili * convertBlMoney).toFixed(3);
                    pls.push(pl);
                });
                var plStr = pls.join('|');
                $("#jiangjin-change").data("value",plStr);

                // 渲染界面中赔率部分
                $("#jiangjin-change").html(plStr);
                $("#jiangjin-change").data("plStr",plStr);
                $('#lt_sel_dyprize').attr('maxfd',plStr);
                $('#lt_sel_dyprize option').val(plStr+'-'+fandianBili+'%');

            }
        });



        //开奖倒计时
        setTimeout(gameKanJiangDataC, 1000, <?=$fptime?>);

        //未来期
        //futureNo('{$actionNo['actionNo']}','{$type}');
        futureNo({$future_phases});
    });

    $(function(){
        // 进来时判断 玩法是否选择正确
        var currListTitle = $('.u_tab_div .tab-front').prevAll('.tab_list_title')
        if (!currListTitle.is('.on')) {
            $('.tab_list_title').removeClass('on')
            currListTitle.addClass('on')
        }


        var currHref = window.location.href;
        var selectIndex = currHref.indexOf('select/');
        if (selectIndex !== -1) {
            var selectArr = currHref.substr(selectIndex + 7).split('-', 3);
            if(selectArr[0] == '{$type}'){
                var selectNumbers = selectArr[1].split(',');
                // 特殊处理
                if (selectArr[0] == 43) {
                    var loadPls = setInterval(function(){
                        // console.log('load....');
                        if (pls) {
                            // 玩法加载好时 选中注单
                            // 香港六合彩   选中特码
                            $('.T-tab a:eq(6)').click();
                            // $('.T-tab a:after').click()

                            // 快钱玩法
                            selectNumbers.forEach(function(elm){
                                var currSelectTd = $('#sscContent table td .ball-' + ((elm -0) < 10 ? '0' + elm : elm)).parent();
                                // 选中
                                currSelectTd.click();
                                // 给值
                                currSelectTd.next().next().find('input').val((selectArr[2]-0)*2);
                            })

                            clearInterval(loadPls)
                        }
                    }, 1000)
                }
                else if(selectArr[0] == 25){
                    var loadPls = setInterval(function(){
                        // console.log('load....');
                        if (pls) {
                            $('.lot-content table td').each(function(index, elm){
                                if (index % 3 == 0) {
                                    if ($.inArray($(this).find('strong').text(), selectNumbers) > -1) {
                                        $(this).click();
                                        $(this).next().next().find('input[type=text]').val((selectArr[2]-0)*2)
                                    }
                                }
                            })

                            clearInterval(loadPls)
                        }
                    }, 1000)
                }
                else{
                    $('#lt_selector .pp').each(function(index, elm){
                        if (selectNumbers.length > index) {
                            $(this).find('.numButton :input[type=button]').eq(selectNumbers[index]).click()
                        }
                    })
                    // 倍数
                    while (selectArr[2] > 1) {
                        $('#plustime').click();
                        selectArr[2] --;
                    }
                }
            }
        }

    })

</script>


