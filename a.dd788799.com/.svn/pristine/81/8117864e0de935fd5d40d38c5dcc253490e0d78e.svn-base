            alert("请输入密码");
            return;
        }
        
        if (!yzm) {
            alert("请输入验证码");
            return;
        }

        
        ajaxRequest({
            url: "/index/login",
            data: {username: loginAccount,password: loginPassword,code:yzm},
            beforeSend: function() {
                showLoading();
            },
            success: function(json) {
                if (json.code == 1) {
                    $.cookie("uid", loginAccount, {path: "/"});
                    $.cookie("token", loginAccount, {path: "/"});
                    window.location.href = "/index/main.html";
                } else {
                    refreshYzm(document.getElementById('registerYzmImg2'));
                    Tools.toast("登录失败：" + json.msg);
