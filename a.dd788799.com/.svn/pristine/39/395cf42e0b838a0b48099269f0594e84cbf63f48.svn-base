@charset "utf-8";
body {
    margin: 0px;
    padding: 0px;
    text-align: center;
    font-family: Verdana, Arial;
    font-size: 12px;
    color: #333333;
    width: 100%;
    background-color: #e1ddda;
}
::-webkit-scrollbar {
    width: 4px;
    height: 10px;
}
::-webkit-scrollbar-thumb {
    /*background-color: rgba(50,64,78,1);*/
}
::-webkit-scrollbar-track {
    /*background-color: rgba(50,64,78,.5);*/
}
::-webkit-scrollbar-thumb {
    min-height: 20px;
    background-clip: content-box;
}
::-webkit-scrollbar-corner {
    background: #002836;
}
* {
    outline: none;
    font-family: 微软雅黑;
}
html div, html span {
    -moz-user-select: -moz-none;
}
textarea {
    resize: none;
}
a, a:link, a:visited, a:active {
    hide-focus: expression(this.hideFocus=true);
    outline: none;
    cursor: pointer;
}
img {
    border: 0px;
    vertical-align: middle;
}
li {
    hide-focus: expression(this.hideFocus=true);
    outline: none;
    list-style-type: none;
    text-align: left;
    line-height: 23px;
}
form {
    margin: 0px;
}
.clear, .c {
    margin: 0px;
    padding: 0px;
    clear: both;
    height: 0px;
    width: 0px;
    font-size: 0px;
    line-height: 0px;
}
h1, h2, h3, h4, h5, p, ul, li {
    margin: 0px;
    padding: 0px;
}
h1 {
    font-size: 24px;
}
h2 {
    font-size: 18px;
}
h3 {
    font-size: 16px;
}
h4 {
    font-size: 14px;
}
h5 {
    font-size: 12px;
}
input {
    vertical-align: middle;
}
.unit {
    /*position: relative;*/
    /*padding-top: 32px;*/
    padding-top: 15px;
    z-index: 1;
    _zoom: 1;
}
.unit_title {
    /*position: absolute;*/
    height: 70px;
    line-height: 40px;
    /*font-weight: bold;*/
    padding-left:9px;
    font-size: 14px;
    width: 100%;
    box-sizing: border-box;
    /*background-image:url('../../images/game/tabheadbg2.jpg');*/
    background-position:left top;
    background-repeat:repeat-x;
    border-radius:10px 10px 0px 0px;
    top: 0;
    left: 0;
    overflow: visible;
    border-bottom:1px #ece2e0 solid;
    margin-top: 10px;
}
.ut_l, .ut_r {
    /*background: url("../../images/game/game_bg.png") repeat-x left -294px;*/
    float: left;
    height: 32px;
    width: 3px;
    display: none;
}
.ut_r {
    float: right;
    background-position: right -294px;
}
#rightcon_h {
    /*background: url("../../images/game/news_bg.gif") repeat-x left -10px;*/
    height: 35px;
    line-height: 32px;
    text-align: left;
    overflow: hidden;
}
#rightcon_h div {
    float: left;
    background: none;
    height: 33px;
    width: 5px;
}
#rightcon_h span {
    display: block;
    color: #ffba01;
    font-size: 12px;
    font-weight: normal;
    height: 33px;
    overflow: hidden;
}
#rightcon_h b {
    color: #fff601;
    font-weight: normal;
}
#rightcon_h a {
    text-decoration: none;
    color: #fff601;
    cursor: pointer;
}
#rightcon {
    overflow-x: hidden;
    overflow-y: auto;
    text-align: left;
}
.game_gundong {
    width: 1000px;
    margin: 0 auto;
    background-color: #f4efec;
    border:1px #bbb5ae solid;
    margin-bottom: 15px;
    border-radius:8px;
    box-shadow: 0px 0px 10px #bbb5ae;
}
.game_rc {
    width: 1024px;
    margin: 0 auto;
    background-color: #f4efec;
    border:1px #bbb5ae solid;
    margin-bottom: 10px;
    border-radius:8px;
    box-shadow: 0px 0px 10px #bbb5ae;
}
.gm_con {
    position: relative;
    _zoom: 1;
    height: 100%;
    padding-bottom: 10px;
    border-radius:10px;
}
.gm_con_lt, .gm_con_rt, .gm_con_lb, .gm_con_rb {
    position: absolute; /*background: url("../../images/game/game_bg.png") no-repeat left top;*/;
    width: 4px;
    height: 4px;
    _line-height: 0;
    _font-size: 0;
}
.gm_con_lt {
    left: 0;
    top: 0;
}
.gm_con_rt {
    background-position: -4px top;
    right: 0;
    /*_right: -1px;*/
    top: 0;
}
.gm_con_lb {
    background-position: left -4px;
    left: 0;
    bottom: 0;
    /*_bottom: -1px;*/
}
.gm_con_rb {
    background-position: -4px -4px;
    right: 0;
    /*_right: -1px;*/
    bottom: 0;
    /*_bottom: -1px;*/
}
.linfo {
    width: 1000px;
    margin: auto;
    height: 90px;
    box-sizing: border-box;
    display: table;
    margin-bottom: 15px;
    margin-top: 15px;
    background-color:#f4efec;
    box-shadow:-1px -1px 10px #bbb5ae;
    border-radius:8px;
    border:1px #bbb5ae solid;
    border-bottom:0;
    /*background-image:url('../../images/game/a.jpg');*/
    background-position:left bottom;
    background-repeat:repeat-x;
}
.linfo > div {
    display: table-cell;
    text-align: center;
    vertical-align: middle;
    padding-bottom:10px;
}
div.linfospace {
