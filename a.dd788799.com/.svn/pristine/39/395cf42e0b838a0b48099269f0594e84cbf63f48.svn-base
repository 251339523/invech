    color:red;
}
.linfomiddle > span >label{
    display:block;
    text-align:right
}
.linforight > div {
    display: inline-block;
    vertical-align: middle;
    color: #744a41;
    text-align:right;
    font-size:14px;
    font-weight:bold;
    margin-right:10px;
}
.linforight div strong {
    display: block;
    font-weight:bold;
    font-size:14px;
}
.linforight div strong>small
{
    color:#ff0000;
    font-size:12px;
    font-weight:normal
}

.linforight div a, .linforight div a:visited {
    display: block;
    padding-left: 10px;
    color: #775b54;
    text-decoration: none;
    height:25px;
    line-height:25px;
    font-weight:normal;
    font-size:12px;
}
.linforight div a img
{
    vertical-align:middle;
    margin-right:3px;
}
.linforight div a:hover {
