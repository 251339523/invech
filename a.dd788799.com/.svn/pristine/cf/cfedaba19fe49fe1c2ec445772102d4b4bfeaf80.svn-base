    function slide_nav(obj,class_name,iname){
        $(obj).parent().hover(function(){
            $(obj).find("i").removeClass(iname);
            $(obj).parent().find(class_name).slideUp();
            $(this).parent().find(class_name).slideDown();
            $(this).find('i').addClass(iname);
            $(obj).attr('open',"0");
            $(this).attr('open',"1");
        },function(){
            //console.log(1);
            $(obj).parent().find(class_name).slideUp();
            $(obj).find('i').removeClass(iname);
            $(obj).attr('open',"0");
        })
    }
