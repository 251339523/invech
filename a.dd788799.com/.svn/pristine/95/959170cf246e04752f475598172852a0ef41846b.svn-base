<script>
    //var pls;//玩法的所有赔率
    //var plg;//当前赔率组

    function changeTab(self) {
        $(self).siblings().removeClass('active');
        $(self).addClass('active');
        var $tabs = $(self).parent().children();

        var index = $tabs.index(self);
        plg = pls[index];

        plg.sz = plg.pls.slice(0, 49);
        plg.sm = plg.pls.slice(49);

        var len = $('.pp').data('line');
        plg.sz.lines = group(plg.sz, len);
        plg.sm.lines = group(plg.sm, len);

        var html = template("six_qbz", {plg: plg});
        $('.table-common').html(html);
    }

    $(function () {
        //循环表格放到table-common
        var playedId = "<?=$playedId?>";
        $.getJSON("/game/pl/playedId/" + playedId, function (data) {
            console.log(data, playedId);
            pls = data;
            $('.T-tab.clearfix').children().eq(0).click();
        });
    });
</script>  