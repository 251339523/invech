    //走势图页面
    function getZstPage(url) {
        if (typeof url == 'undefined') {
            url = 'index';
        }
        window.open("/ssc/zst/" + url + ".html?type=1");
    }

    function getSscPage(url) {
        parent.getSscPage(url);
    }

    var getOpenCodeHistoryFlagArr = [];
