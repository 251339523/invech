<script>
    $(function() {
        // 底部信息调用
        function ajaxGetDbxx() {
            ajaxRequest({
                url: CONFIG.BASEURL + "ajaxGetWebInfo.json",
                success: function (json) {
                    if (json.result != 1) {
                        return;
                    }
                    $(".webDbxx").html(json.webDbxx);
                }
            });
        }

        if ($(".webDbxx").length > 0) {
            ajaxGetDbxx();
        }
    });
</script><script src="/static/theme/memberTheme/theme1/member/js/index.js?v=20170803_1"></script>
<script>
    var CONFIG = {
        BASEURL: "/",    // 根路径1
