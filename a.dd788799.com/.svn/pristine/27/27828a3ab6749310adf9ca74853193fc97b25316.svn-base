/*** overwrite by feiker ***/
.current_play_wp .current_play_btn_wp p{
	font-size: 12px;
	text-align: left;
	margin: 0;
	margin-left:10px;
}
.current_play_wp ul{
	display: block;
	width: 100%;
	margin-left:10px;
}
.current_play_wp ul li{
	display: inline-block;
	float: left;
	width: 21px;
	height: 21px;
	background: #fbf7f5;
	margin: 5px 4px;
	margin-left: 0;
	color: #ff6a10;
	line-height: 17px;
	text-align: center;
	border: 2px solid #f4efec;
	border-radius: 50%;
	font-weight: bold;
}
.current_play_wp button{
	width: 32px;
	height: 32px;
	position: absolute;
	right: 15px;
	bottom: 3px;
	border: none;
	background: none;
	outline: none;
	color: #999;
	font-size: 20px;
	text-align: center;
	line-height: 32px;
}
.current_play_time_left{
	width: 100%;
	height: 30px;
	margin-top: 0;
	line-height: 30px;
	border-bottom: 1px solid #e7e7e7;
	box-shadow: 2px 2px 2px #e7e7e7;
}
.current_play_time_left h3{
	margin-top:0;
	font-size:12px;
	color:#666;
	float: left;
}
.current_play_time_left h3 span#count_down{
	color:#FF4200;
}
.current_play_time_left p{
	font-size:12px;
	color:#666;
}
.current_play_time_left p button{
	width: 30px;
	height: 30px;
	display: inline-block;
	text-align: center;
	line-height: 30px;
	color: #FF4200;
	position: static;
	font-size: 12px;
	padding: 0;
}

.current_play_wp .current_play_btn_wp p span{
	position: static;
	font-size: 12px;
}
.current_play_wp .current_play_btn_wp p strong{
	font-weight: normal;
}
.play_group{
    display: none;
    width: 100%;
	background: rgba(0,0,0,0.6);
    position: absolute;
    top: 141px;
    z-index: 555;
    border-bottom: 1px solid lightgray;
    font-size: 14px;
}
.play_group ul{
    display: inline-block;
    width: 50%;
    float: left;
	background: #fff;
}
.play_group ul li{
    /*border-bottom: 1px solid lightgray;*/
    line-height: 26px;
    text-align: center;
    border-right: 1px solid lightgray;
}
.play_group ul li:last-child{
    border-bottom:none;
}
.play_group ul:last-child li{
    border-right:none;
}