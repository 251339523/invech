<?php
namespace app\mobile\model;

use think\Model;

/**
 * 系统配置表 模型定义
 * Class Params
 * @package app\admin\model
 */
Class Params extends Model
{

    /**
     * 获取一条数据
     * @param $id
     * @return $this|mixed
     */
    public function getOne($id)
    {
        if (!empty($id)) {
            return $this->find($id);
        }
    }
}