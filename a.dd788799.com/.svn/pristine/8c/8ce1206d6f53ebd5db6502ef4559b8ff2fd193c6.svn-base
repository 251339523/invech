        <table>
            <thead>
            <tr>
              <th>class</th>
              <th>property</th>
              <th>code</th>
              <th>msg</th>
            </tr>
            </thead>
            <tbody>
                {foreach($errors as $error)}
                <tr>
                  <td>{$error.class}</td>
                  <td>{$error.property}</td>
                  <td>{$error.code}</td>
                  <td>{$error.msg}</td>
                </tr>
                {/foreach}
            </tbody>
        </table>
            
        </p>
