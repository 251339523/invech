/*皇冠新增样式 by thomas lee -- 2018.01.20*/
body,html{font-family: 'Microsoft YaHei',"微软雅黑",SimSun,sans-serif;}
.menu li i{display: block;width: 100%;text-align: center;font-size: 25px;height: 27px;line-height: 27px;}
.menu a .iconfont{color: white;}
.menu a.on{color:#f1f917;}
.menu a.on .iconfont{color:#f1f917;}
.menu a{color: white;text-align: center;font-size: 14px;}
.menu ul li{text-align: center;padding: 2px 0px;}
.menu { position: fixed; width: 100%; bottom: 0; left: 0; right: 0; z-index: 6; margin:auto; background: #e64e51;height: 50px; }
.menu ul { margin:auto; padding: 0;}
.menu ul li { width:20%; float: left;list-style: none;}
.menu a img { max-width: 100%; height: auto; }