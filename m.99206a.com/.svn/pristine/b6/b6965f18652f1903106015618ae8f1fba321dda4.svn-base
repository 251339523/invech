.date_pricker_wp{
  width: 1178px;
  height: 48px;
  background: #ffffff;
  border: 1px solid #dbdbdb;
  border-top:none;
  border-bottom: none;
}
.data-pricker-form{
  width: 100%;
  clear: both;
}
.data-select{
  color: #000;
  width: 187px;
  float: left;
}
.data-select-label{
  line-height: 48px;
  font-size: 12px;
  margin-left: 20px;
}
.data-select-option{
  width: 98px;
  height: 27px;
  border: 1px solid #efefef;
  border-radius: 4px;
  text-align: center;
  outline: none;
  padding-left: 5px;
}
.data-select-option option{
  text-align: center;
  display: block;
}
.lottery_time{
    color: #000;
    padding-top: 11px;
}
.lottery_time span{
    display: block;
    float: left;
    height: 27px;
    line-height: 27px;
    margin-left: 5px;
    margin-right: 5px;
}
.lottery_time button{
    display: block;
    float: left;
    width: 68px;
    height: 27px;
    background: #ff6c00;
    color: #ffffff;
    text-align: center;
    line-height: 27px;
    outline: none;
    border-radius: 8px;
    border: none;
    cursor: pointer;
    margin-left: 10px;
}
.lottery_time button:hover{
    background: #d86714;
}
.start_time_input{
    width: 82px;
    height: 27px;
    border: 1px solid #a4a4a4;
    border-radius: 8px;
    text-align: center;
    display: block;
    float: left;
    outline: none;
    line-height: 27px;
}
.today-time{
    float: right;
}
.today-time span{
  color: #000;
  display: block;
  float: left;
  width: 63px;
  height: 26px;
  background: #f8f8f6;
  border: 1px solid #d2d2d2;
  border-radius: 15px;
  text-align: center;
  line-height: 26px;
  cursor: pointer;
  margin-right:10px;
}
.today-time span:hover{background:#ff6c00;color: white;border: 1px solid #ff6c00;}

.today-time span.today-time-bg{
  background: #ff6c00;
  color: #fff;
}

.paging_cont_page li.active{
    background: #262522;
    color: #ffffff;
}
.paging_cont_page a{
    color: #262522;
}

.modify_main_cont{
    padding: 8px;
}
.modify_main_cont_font{
    display: none;
}
.modify_main_cont_table{
    width: 100%;
    border-color:#7d010f;
    text-align: center;
    border-collapse:collapse;
}


.list_table_wp{
    width: 1182px;
    border-top:1px solid rgb(219, 219, 219);
    background: #ffffff;
    overflow: hidden;
}
.list_table_title{
    width: 100%;
    height: 32px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
}
.list_table_title span{
    display: block;
    float: left;
    width: 16.55%;
    text-align: center;
    line-height: 32px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    background: #f8f8f6;
    font-size: 14px;
}

.list_Tbody_wp{
    width: 100%;
    overflow: hidden;
}
.list_Tbody_wp li{
    float: left;
    width: 100%;
    color: #000000;
    border: 1px solid #dbdbdb;
    border-bottom: none;
    border-right:none;
    cursor: pointer;
}
.list_Tbody_wp li.li_add_border_d{
    border-style: dashed;
    border-color: #ff7433;
    border-bottom: none;
}
.list_Tbody_wp li.li_add_border_top{
    border-top-style:dashed;
    border-top-color: #ff7433;
}
.list_Tbody_wp li span{
    display: block;
    width: 16.55%;
    float: left;
    height: 100%;
    text-align: center;
    border-right:1px solid #dbdbdb;
}
.list_Tbody_wp li span i{
    display: block;
    font-style: normal;
    margin-top: 5px;
}
.list_Tbody_wp li span b.list_Tbody_time{
    display: block;
    font-weight: normal;
}
.list_Tbody_wp li span b.list_Tbody_totle{
    display: block;
    font-weight: normal;
    line-height: 42px;
    color:#007eff;
    font-size: 13px;
    /*border-right: 1px solid #d2d2d2;*/
}
.list_Tbody_wp li span b.list_Tbody_totle.list_Tbody_totle_a{
    color: blue;
}
.list_Tbody_wp li span b.list_Tbody_totle.list_Tbody_totle_right{
    border-right: 1px dashed #ff7433;
}
.list_Tbody_wp li span em{
    line-height: 42px;
    font-style: normal;
}
.list_Tbody_wp li span a{
    line-height: 42px;
    color: #000000;
}
.list_table_tfooter{
    width: 1178px;
    height: 44px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
    border-bottom: 1px solid #dbdbdb;
    clear: both;
    background: #fffcdf;
}
.list_table_tfooter span{
    display: block;
    float: left;
    width: 16.55%;
    text-align: center;
    line-height: 44px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    font-size: 14px;
}
.list_table_tfooter span a{
    color: #000000;
}

.table_button_wp{
    width: 99.8%;
    clear: both;
    height: 198px;
    border: 1px solid #dbdbdb;
    border-top:none;
}
.table_button_wp .button_fist{
    margin-left: 395px;
}
.table_button_wp button{
    display: block;
    float: left;
    width: 90px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    background: #f8f8f6;
    border: 1px solid #dbdbdb;
    outline: none;
    border-radius: 30px;
    margin-right: 10px;
    cursor: pointer;
    margin-top: 98px;
}
.table_button_wp button:hover{
    background: #262522;
    color: #ffffff;
}
.table_button_wp span{
    color: #000000;
    line-height: 26px;
    float: left;
    margin-top: 98px;
}




.paging_wp{
    width: 100%;
    height: 33px;
    margin-top: 30px;
    position: relative;
}
.paging{
    margin: 0 auto;
    height: 100%;
}
.prev_btn{
    width: 30px;
    height: 30px;
    float: left;
    cursor: pointer;
}

.prev_btn img{
    display: block;
    width: 100%;
    height: 100%;
    margin-right: 5px;
}

.paging_cont_page{
    float: left;
}
.paging_cont_page li{
    display: inline-block;
    width: 28px;
    height: 28px;
    background: #ffffff;
    border: 1px solid #262522;
    border-radius: 3px;
    margin-left: 5px;
    text-align: center;
    line-height: 28px;
    color: #262522;
    font-size: 16px;
}
.paging_cont_page li:hover{background: #262522;color: white;}

.paging_cont_page li.add_background_a{
    background: #262522;
    color: #ffffff;
}
.next_btn{
    width: 30px;
    height: 30px;
    float: left;
    margin-left: 5px;
    cursor: pointer;
}
.next_btn img{
    display: block;
    width: 100%;
    height: 100%;
}
.prve-pages:hover{
  background: #ff4e00;
}
.show-page{
  position: absolute;
  right:0px;
  top:0px;
  color: #000;
}
