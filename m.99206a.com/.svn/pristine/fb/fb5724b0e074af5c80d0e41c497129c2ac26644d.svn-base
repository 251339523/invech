        if(data==""){
        }else{
         $data_arr=data.split("||");
		 $("#user_money").html($data_arr[0]);
		 // $("#user_jifen").html($data_arr[1]);
        }
    }
}); 
window.setTimeout("refresh_money();", 5000); 
}
refresh_money();
