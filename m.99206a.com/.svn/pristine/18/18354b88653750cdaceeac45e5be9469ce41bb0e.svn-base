<script type="text/javascript">
$(function(){
$('#line_2_a_up').click(function(){
	$('#cesu').find('.line_ms_1 i').remove()
	test()
})
$('#line_2_a_down').click(function(){
	$('#cesu_2').find('.line_ms_1 i').remove()
	test_2()
})

})

//JS  PING网址
function test() {
	var span,
		d = document,
		li,
		a,
		i,
		lis = d.getElementById('cesu').getElementsByTagName('li'),
		lis_2 = d.getElementById('cesu_2').getElementsByTagName('li')
		
	for (i = 0; li = lis[i++]; ) {
		a = li.getElementsByTagName('a')[0];
		if (!a) { continue; }
		span = d.createElement('i');
		span.ctime=new Date();
		span.innerHTML = '测速中...<img src="' + a.href + '" border="0" width="1" height="1" onerror="testresult(this)" />';
		li.appendChild(span);
	}

	for (i = 0; i< $('#cesu').find('li.cesu_li').length; i++ ) {
		var bbbbb = $('#cesu').find('li.cesu_li').eq(i).find('i');
		var ccccc = $('#cesu').find('li.cesu_li').eq(i).find('.line_ms_1');
		bbbbb.appendTo(ccccc).end()
	}
}

function test_2() {
	var span,
		d = document,
		li,
		a,
		i,
		lis = d.getElementById('cesu').getElementsByTagName('li'),
		lis_2 = d.getElementById('cesu_2').getElementsByTagName('li')
		
	for (i = 0; li = lis_2[i++]; ) {
		a = li.getElementsByTagName('a')[0];
		if (!a) { continue; }
		span = d.createElement('i');
		span.ctime=new Date();
		span.innerHTML = '测速中...<img src="' + a.href + '" border="0" width="1" height="1" onerror="testresult(this)" />';
		li.appendChild(span);
	}

	for (i = 0; i< $('#cesu_2').find('li.cesu_li').length; i++ ) {
		var bbbbb = $('#cesu_2').find('li.cesu_li').eq(i).find('i');
		var ccccc = $('#cesu_2').find('li.cesu_li').eq(i).find('.line_ms_1');
		bbbbb.appendTo(ccccc).end()
	}
}

function testresult(img) {
	var span = img.parentNode;
	var n = 'em';
	if (!testresult.isrun) { n = 'span';  testresult.isrun = true; }
	span.innerHTML = '<' + n + '>' + parseInt((new Date() - span.ctime)/10 ) + 'ms</' + n + '>';
};
var ran = Math.random();
test();
test_2();

</script>
