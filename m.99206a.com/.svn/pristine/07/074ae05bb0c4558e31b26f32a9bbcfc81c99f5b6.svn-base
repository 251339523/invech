.modify_wp{
    width: 1177px;
    height: auto;
    margin: 30px auto;
}

.modify_header{
    width: 1179px;
    height: 70px;
    background: #262522;
}
.modify_header a{
    display: block;
    float: left;
    width: 56px;
    height: 50px;
    color: transparent;
    margin-top: 15px;
    margin-left: 26px;
}
.modify_header a:hover{
    background-position: top center;
}
.modify_header .nav_1{
    background: url("../img/nav_1.png") no-repeat bottom center;
}
.modify_header .nav_2{
    background: url("../img/nav_2.png") no-repeat bottom center;
}
.modify_header .nav_3{
    background: url("../img/nav_3.png") no-repeat bottom center;
}
.modify_header .nav_4{
    background: url("../img/nav_4.png") no-repeat bottom center;
}
.modify_header .nav_5{
    background: url("../img/nav_5.png") no-repeat bottom center;
}
.modify_header .nav_6{
    background: url("../img/nav_6.png") no-repeat bottom center;
}

.modify_main_wp{
    width: 1179px;
    height: 525px;
    border: 1px solid #dbdbdb;
    margin-top: 10px;
}

.modify_main_btn{
    width: 100%;
    height: 37px;
    background: #f8f8f6;
    border-bottom: 1px solid #dbdbdb;
}
.modify_main_btn a{
    width: 107px;
    height: 36px;
    text-align: center;
    display: block;
    float: left;
    line-height: 34px;
    font-size: 16px;
    color: #000000;
    border-top:3px solid #f8f8f6;
    border-right:1px solid #e5e5e5;
}
.modify_main_btn a.click_A{
    border-top:3px solid #2d3036;
    height: 35px;
    background: #ffffff;
    border-bottom: 0px;
}


.list_table_wp{
    width: 1179px;
    border-top:1px solid rgb(219, 219, 219);
    background: #ffffff;
    overflow: hidden;
    margin-top: 10px;
}
.list_table_title{
    width: 100%;
    height: 32px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
}
.list_table_title span{
    display: block;
    float: left;
    width: 24.88%;
    text-align: center;
    line-height: 32px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    background: #f8f8f6;
    font-size: 14px;
}

.list_Tbody_wp{
    width: 100%;
    overflow: hidden;
}
.list_Tbody_wp li{
    float: left;
    width: 100%;
    height: 42px;
    color: #000000;
    border: 1px solid #dbdbdb;
    border-bottom: none;
    border-right:none;
    cursor: pointer;
}
.list_Tbody_wp li.li_add_border_d{
    border-style: dashed;
    border-color: #ff7433;
    border-bottom: none;
}
.list_Tbody_wp li.li_add_border_top{
    border-top-style:dashed;
    border-top-color: #ff7433;
}
.list_Tbody_wp li span{
    display: block;
      width: 24.88%;
    float: left;
    height: 100%;
    text-align: center;
    border-right:1px solid #dbdbdb;
}
.list_Tbody_wp li span i{
    display: block;
    font-style: normal;
    margin-top: 5px;
}
.list_Tbody_wp li span b.list_Tbody_time{
    display: block;
    font-weight: normal;
}
.list_Tbody_wp li span b.list_Tbody_totle{
    display: block;
    font-weight: normal;
    line-height: 42px;
    /*border-right: 1px solid #d2d2d2;*/
}
.list_Tbody_wp li span b.list_Tbody_totle.list_Tbody_totle_right{
    border-right: 1px dashed #ff7433;
}
.list_Tbody_wp li span em{
    line-height: 42px;
    font-style: normal;
}
.list_Tbody_wp li span a{
    line-height: 42px;
    color: #000000;
}
.list_table_tfooter{
    width: 100%;
    height: 44px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
    border-bottom: 1px solid #dbdbdb;
    clear: both;
    background: #fffcdf;
}
.list_table_tfooter span{
    display: block;
    float: left;
      width: 24.88%;
    text-align: center;
    line-height: 44px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    font-size: 14px;
}
.list_table_tfooter span a{
    color: #000000;
}

.table_button_wp{
    width: 99.8%;
    clear: both;
    height: 198px;
    border: 1px solid #dbdbdb;
    border-top:none;
}
.table_button_wp .button_fist{
    margin-left: 395px;
}
.table_button_wp button{
    display: block;
    float: left;
    width: 90px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    background: #f8f8f6;
    border: 1px solid #dbdbdb;
    outline: none;
    border-radius: 30px;
    margin-right: 10px;
    cursor: pointer;
    margin-top: 98px;
}
.table_button_wp button:hover{
    background: #262522;
    color: #ffffff;
}
.table_button_wp span{
    color: #000000;
    line-height: 26px;
    float: left;
    margin-top: 98px;
}




.paging_wp{
    width: 100%;
    height: 33px;
    margin-top: 30px;
    position: relative;
}
.paging{
    width: 258px;
    margin: 0 auto;
    height: 100%;
}
.prev_btn{
    width: 30px;
    height: 30px;
    float: left;
    cursor: pointer;
}

.prev_btn img{
    display: block;
    width: 100%;
    height: 100%;
    margin-right: 5px;
}

.paging_cont_page{
    float: left;
}
.paging_cont_page a{
    display: inline-block;
    width: 28px;
    height: 28px;
    background: #ffffff;
    border: 1px solid #262522;
    border-radius: 3px;
    margin-left: 5px;
    text-align: center;
    line-height: 28px;
    color: #262522;
    font-size: 16px;
}
.paging_cont_page a:hover{background: #262522;color: white;}
.paging_cont_page a.add_background_a{
    background: #262522;
    color: #ffffff;
}
.next_btn{
    width: 30px;
    height: 30px;
    float: left;
    margin-left: 5px;
    cursor: pointer;
}
.next_btn img{
    display: block;
    width: 100%;
    height: 100%;
}
.prve-pages:hover{
  background: #ff4e00;
}
.show-page{
  position: absolute;
  right:0px;
  top:0px;
  color: #000;
}
