.modify_wp{
    width: 1179px;
    height: auto;
    margin: 30px auto;
}

.modify_header{
    width: 1179px;
    height: 70px;
    background: #262522;
}
.modify_header a{
    display: block;
    float: left;
    width: 56px;
    height: 50px;
    color: transparent;
    margin-top: 15px;
    margin-left: 26px;
}
.modify_header a:hover{
    background-position: top center;
}
.modify_header .nav_1{
    background: url("../img/nav_1.png") no-repeat bottom center;
}
.modify_header .nav_2{
    background: url("../img/nav_2.png") no-repeat bottom center;
}
.modify_header .nav_3{
    background: url("../img/nav_3.png") no-repeat bottom center;
}
.modify_header .nav_4{
    background: url("../img/nav_4.png") no-repeat bottom center;
}
.modify_header .nav_5{
    background: url("../img/nav_5.png") no-repeat bottom center;
}
.modify_header .nav_6{
    background: url("../img/nav_6.png") no-repeat bottom center;
}

.modify_main_wp{
    width: 1179px;
    height: 525px;
    border: 1px solid #dbdbdb;
    margin-top: 10px;
}

.modify_main_btn{
    width: 100%;
    height: 37px;
    background: #f8f8f6;
    border-bottom: 1px solid #dbdbdb;
}
.modify_main_btn a{
    width: 107px;
    height: 36px;
    text-align: center;
    display: block;
    float: left;
    line-height: 34px;
    font-size: 16px;
    color: #000000;
    border-top:3px solid #f8f8f6;
    border-right:1px solid #e5e5e5;
}
.modify_main_btn a.click_A{
    border-top:3px solid #2d3036;
    height: 35px;
    background: #ffffff;
    border-bottom: 0px;
}



.modify_main_btn{
    width: 100%;
    height: 37px;
    background: #f8f8f6;

    border-bottom: 1px solid #dbdbdb;
}
.modify_main_btn a{
    width: 107px;
    height: 36px;
    text-align: center;
    display: block;
    float: left;
    line-height: 34px;
    font-size: 16px;
    color: #000000;
    border-top:3px solid #f8f8f6;
    border-right:1px solid #e5e5e5;
}
.modify_main_btn a.click_A{
    border-top:3px solid #2d3036;
    height: 35px;
    background: #ffffff;
    border-bottom: 0px;
}


.enter-input-wp{
  width: 960px;
  margin: 10px auto;
  clear: both;
}
.enter-input-wp p{
  float: left;
  margin-right: 8px;
  margin-top: 15px;
}
.enter-input{
  width: 347px;
  height: 42px;
  border: 1px solid #c6c6c6;
  padding: 2px 5px;
}
.enter-input label{
  display: block;
  float: left;
  height: 100%;
  border-right:1px solid #c6c6c6;
  color: #333333;
  line-height: 42px;
  font-size: 14px;
  width: 104px;
  text-align: center;
}
.enter-input select{
  display: block;
  float: left;
  width: 226px;
  height: 100%;
  border: none;
  outline: none;
  text-align: center;
  color: #666;
  margin-left: 10px;
}
.enter-input select option{
  text-align: center;
}
.enter-input input{
  display: block;
  float: left;
  width: 226px;
  height: 100%;
  border: none;
  outline: none;
  color: #666;
  margin-left: 10px;
}
.enter-btn{
  width: 200px;
  height: 40px;
  background:#ff6c00;
  color: #fff;
  text-align: center;
  line-height: 40px;
  border-radius: 30px;
  margin-top: 4px;
  margin-left: 15px;
  cursor: pointer;
  font-size: 16px;
}
.enter-btn:hover{background: #d86714;}
.enter-btn.enter-btn-exit:hover{background: #0795fb;}

.enter-btn-exit{
  background: #45b1ff;
}
.tip-font{
  clear: both;
  padding-left: 30px;
  color: #666;
  height: 60px;
  line-height: 60px;
}
.tip-font span{
  color: #ff6c00;
}



.pay-my{

}
.pay{
  width: 230px;
  height: 47px;
  border: 1px solid #c6c6c6;
  margin-left: 10px;
  color: #333;
  float: left;
  margin-bottom: 15px;
  cursor: pointer;
}
.pay:hover{background: #fddcc3;border: 1px solid #ff6c00}
.pay span{
  display: block;
  float: left;
  line-height: 47px;
  margin-left: 15px;
  font-size: 14px;
}
.pay p{
  float: right;
}
.pay p b{
  color: #ff6c00;
  font-size: 16px;
  padding-top: 15px;
  margin-right: 8px;
  display: block;
  float: left;
}
.pay p img{
  margin-right: 10px;
  cursor: pointer;
  width: 16px;
  height: 16px;
  cursor: pointer;
  display: block;
  float: left;
  margin-top: 16px;
}
.pay-btn{
  width: 200px;
  height: 40px;
  background: #45b1ff;
  float: left;
  border-radius: 30px;
  text-align: center;
  margin-left: 20px;
}
.pay-btn:hover{background: #0795fb;}
.pay-btn img{
  display: block;
  float: left;
  margin-left: 30px;
  margin-top: 10px;
}
.pay-btn span{
  float: left;
  display: block;
  line-height: 40px;
  font-size: 16px;
  margin-left: 10px;
  cursor: pointer;
  margin-bottom: 15px;
}
