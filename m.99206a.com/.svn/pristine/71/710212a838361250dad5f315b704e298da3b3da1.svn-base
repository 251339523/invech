.modify_main_btn{
    width: 100%;
    height: 37px;
    background: #f8f8f6;
    margin-top: 10px;
    overflow: hidden;
    border:1px solid rgb(219, 219, 219);
    border-bottom:none;
}
.modify_main_btn a{
    height: 36px;
    text-align: center;
    display: block;
    float: left;
    line-height: 34px;
    font-size: 14px;
    color: #000000;
    margin-left: 15px;
}
.modify_main_btn p{
    height: 36px;
    text-align: center;
    display: block;
    float: right;
    line-height: 34px;
    font-size: 14px;
    color: #000000;
    padding-right: 15px;
}


.modify_wp{
    width: 977px;
    height: auto;
    margin: 30px auto;
}




.date_pricker_wp{
  width: 100%;
  height: 48px;
  background: #ffffff;
  border: 1px solid #dbdbdb;
  border-top:none;
  border-bottom: none;
}
.data-pricker-form{
  width: 100%;
  clear: both;
}
.data-select{
  color: #000;
  width: 187px;
  float: left;
}
.data-select-label{
  line-height: 48px;
  font-size: 12px;
  margin-left: 20px;
}
.data-select-option{
  width: 98px;
  height: 27px;
  border: 1px solid #efefef;
  border-radius: 4px;
  text-align: center;
  outline: none;
  padding-left: 5px;
}
.data-select-option option{
  text-align: center;
  display: block;
}
.lottery_time{
    color: #000;
    padding-top: 11px;
}
.lottery_time span{
    display: block;
    float: left;
    height: 27px;
    line-height: 27px;
    margin-left: 5px;
    margin-right: 5px;
}
.lottery_time button{
    display: block;
    float: left;
    width: 68px;
    height: 27px;
    background: #ff6c00;
    color: #ffffff;
    text-align: center;
    line-height: 27px;
    outline: none;
    border-radius: 8px;
    border: none;
    cursor: pointer;
    margin-left: 10px;
}
.lottery_time button:hover{
    background: #f23d08;
}
.start_time_input{
    width: 82px;
    height: 27px;
    border: 1px solid #a4a4a4;
    border-radius: 8px;
    text-align: center;
    display: block;
    float: left;
    outline: none;
    line-height: 27px;
}
.today-time{
    float: right;
}
.today-time span{
  color: #000;
  display: block;
  float: left;
  width: 63px;
  height: 26px;
  background: #f8f8f6;
  border: 1px solid #d2d2d2;
  border-radius: 15px;
  text-align: center;
  line-height: 26px;
  cursor: pointer;
  margin-right:10px;
}
.today-time span.today-time-bg{
  background: #ff6c00;
  color: #fff;
}





.modify_main_cont{
    padding: 8px;
}
.modify_main_cont_font{
    display: none;
}
.modify_main_cont_table{
    width: 100%;
    border-color:#7d010f;
    text-align: center;
    border-collapse:collapse;
}


.ul_wp{
    width: 947px;
    border:1px solid rgb(219, 219, 219);
    background: #ffffff;
    overflow: hidden;
    padding: 15px;
    font-size: 14px;
    color: #333;
    clear: both;
}
.ul_wp li{
  line-height: 25px;
}



.btn-wp{
  width: 315px;
  margin: 15px auto;
}
.btn-wp a{
  display: block;
  float: left;
  width: 82px;
  height: 28px;
  border: 1px solid #3333;
  color: #333;
  text-align: center;
  line-height: 28px;
  font-size: 14px;
  margin-left: 18px;
}
.btn-wp a:hover{background: #d86714;color: white;}
.btn-wp a.btn-wp-bg{
  background: #ff6c00;
  color: #fff;
  /*border:1px solid #ff6c00;*/
}
.btn-wp a.btn-wp-bg:hover{background: #d86714;}
