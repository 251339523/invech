/* 首页全屏大幻灯片 */
.fullSlide{ width:100%;position:relative;height:500px; background:#000;z-index: 1000;}
.fullSlide .bd{ margin:0 auto; position:relative; z-index:0; overflow:hidden;}
.fullSlide .bd ul{width:100% !important;}
.fullSlide .bd li{width:100% !important;height:500px; overflow:hidden; text-align:center;}
.fullSlide .bd li a{display:block;height:500px;}
.fullSlide .hd{width:100%;position:absolute;z-index:1;bottom:10px;left:0; height:30px;line-height:30px;}
.fullSlide .hd ul{ text-align:center;}
.fullSlide .hd ul li{ cursor:pointer; display:inline-block; *display:inline; zoom:1; width:15px; height:15px; margin:1px 5px; overflow:hidden;background:#000;filter:alpha(opacity=50);opacity:0.5;  line-height:999px;border-radius: 50%;}
.fullSlide .hd ul .on{ background:#fff;}
.fullSlide .prev,
.fullSlide .next{display:block;position:absolute;z-index:1;top:50%;margin-top:-30px; left:15%;z-index:1; width:40px; height:60px;background:url(../img/slider-arrow.png) -126px -137px #000 no-repeat;  cursor:pointer; filter:alpha(opacity=50);opacity:0.5; display:none;}
.fullSlide .next{ left:auto; right:15%; background-position:-6px -137px;}
