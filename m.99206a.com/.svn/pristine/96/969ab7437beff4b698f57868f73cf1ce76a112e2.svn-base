		$(window.frames["leftFrame"].document).find("#button_chuanguan").click();


	});

	//点击第一行滚球获取index()
	$(".deal_list_top div").click(function(){
		var index = $(this).index();
		$(".deal_bottom div").eq(index).show().siblings("div").hide();
	});

	//点击第一行今日赛事获取index()
	$(".deal_list_top_2 div").click(function(){
		var index2 = $(this).index();
		$(".deal_bottom,.deal_list_bottom_jinri").hide();

		if (index2 == 0){
			$(".deal_list_bottom_2").show().siblings("div").hide();
		} else if(index2 == 1){
			$(".deal_list_bottom_3").show().siblings("div").hide();
		} else if(index2 == 2){
			$(".deal_list_bottom_4").show().siblings("div").hide();
		} else if(index2 == 3){
			$(".deal_list_bottom_5").show().siblings("div").hide();
		} else if(index2 == 4){
			$(".deal_list_bottom_6").show().siblings("div").hide();
		} else if(index2 == 5){
			$(".deal_list_bottom_7").show().siblings("div").hide();
		} else{}

	});

	//点击第一行早盘获取index()
	$(".deal_list_top_3 div").click(function(){
		var index3 = $(this).index();
		$(".deal_list_bottom_jinri").show();
		$(".deal_bottom,.deal_toady").hide();

		if(index3 == 0){
			$("#Label1").show();
			$("#Label3").hide();
		} else{
			$("#Label3").show();
			$("#Label1").hide();
		}

	});

	//点击第一行串关投注获取index()
	$(".deal_list_top_5 div").click(function(){
		var index5 = $(this).index();
		$(".deal_list_bottom_chuanguan").show();
		$(".deal_bottom,.deal_toady,.deal_list_bottom_jinri").hide();

		if(index5 == 0){
			$("#Label11").show();
			$("#Label12").hide();
		} else{
			$("#Label12").show();
			$("#Label11").hide();
		}

	});

	//无缝上滚动
	$(".txtMarquee-top").slide({mainCell:".bd ul",autoPlay:true,effect:"topMarquee",vis:1,interTime:50,trigger:"click"});

	//点击变色
	$(".deal_list_top div,.deal_list_top_2 div,.deal_list_top_3 div").click(function(){
		$(this).css("color","#f9c100").siblings().css("color","#ffffff");
	});

	// 时间
	setInterval(function () {
        t+=1000;
        //console.log(t);
        var _t = new Date(t);
        var _time = {
			year: _t.getFullYear(),
			month: parseInt(_t.getMonth())+1<10 ? '0'+(parseInt(_t.getMonth())+1) : parseInt(_t.getMonth())+1,
			day: parseInt(_t.getDate())<10 ? '0'+_t.getDate() : _t.getDate(),
			hours:parseInt(_t.getHours())<10 ? '0'+ _t.getHours():_t.getHours(),
			minutes:parseInt(_t.getMinutes())<10 ? '0'+_t.getMinutes():_t.getMinutes(),
			seconds:parseInt(_t.getSeconds())<10 ? '0'+_t.getSeconds():_t.getSeconds()
		};
        var fullTime = _time.year+'-'+_time.month+'-'+_time.day+' '+_time.hours+':'+_time.minutes+':'+_time.seconds;
		//console.log(_time.year,_time.month,_time.day,_time.hours,_time.minutes,_time.seconds);
		$('.admin_nav_left ul li:last-child').text(fullTime);
    },1000);

})

</script>

