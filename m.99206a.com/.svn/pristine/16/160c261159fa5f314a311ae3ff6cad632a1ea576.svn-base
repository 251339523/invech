.wap_500f_lottery{width: 100%;height: auto;background: white;font-size: 0;}
.wap_500f_lottery_1{width: 33.1%;height: 140px;display: inline-block;font-size: 16px;text-align: center;border-left: 1px solid #e5e5e5;border-bottom: 1px solid #E5E5E5;}
