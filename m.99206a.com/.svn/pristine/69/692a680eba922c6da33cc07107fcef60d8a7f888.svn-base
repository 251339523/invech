
.modify_wp{
  width: 1179px;
  height: auto;
  margin: 30px auto;
}

.paging_wp{
    width: 100%;
    height: 33px;
    margin-top: 30px;
}
.paging{
    width: 258px;
    margin: 0 auto;
    height: 100%;
}
.prev_btn{
    width: 30px;
    height: 30px;
    float: left;
    cursor: pointer;
}

.prev_btn img{
    display: block;
    width: 100%;
    height: 100%;
    margin-right: 5px;
}




.paging_cont_page{
    float: left;
}
.paging_cont_page a{
    display: inline-block;
    width: 28px;
    height: 28px;
    background: #ffffff;
    border: 1px solid #262522;
    border-radius: 3px;
    margin-left: 5px;
    text-align: center;
    line-height: 28px;
    color: #262522;
    font-size: 16px;
}
.paging_cont_page a.add_background_a{
    background: #262522;
    color: #ffffff;
}
.next_btn{
    width: 30px;
    height: 30px;
    float: left;
    margin-left: 5px;
    cursor: pointer;
}
.next_btn img{
    display: block;
    width: 100%;
    height: 100%;
}
.prve-pages:hover{
  background: #ff4e00;
}
