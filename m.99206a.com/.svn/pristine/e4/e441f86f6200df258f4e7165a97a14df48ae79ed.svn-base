.nav_show a{margin:0px;display: block;text-align: center;background: #000000;border-top: 1px solid #ffc70e;padding: 8px 0px;
	filter:alpha(opacity=80);
	-moz-opacity:0.8;
	-khtml-opacity: 0.8; 
	opacity:0.8; 	
}
.nav_show a:first-child{border-top: none;}
.nav_show a:hover{background: #555555;}
/* 首页全屏带进度条大轮播图 */
.index-main-box{position:relative;width:100%;overflow:hidden;height:465px;
  -webkit-box-shadow:2px 3px 3px #999;  
  -moz-box-shadow:2px 3px 3px #999;  
  box-shadow:2px 3px 3px #999; 
}
#indexslider{position:relative;width:100%;height:465px;}
.index-img{display:block;width:100%;height:465px;overflow:hidden;}
.index-img img{width:100%;height:465px;display: block;}
.index-pagination{position:absolute;z-index:66;top:400px;left:50%;height:30px;width:240px;margin-left:-120px;text-align:center;}
.index-pagination li{display:inline-block;width:50px;position:relative;float:left;height:8px;margin-right:8px;overflow:hidden;}
.index-pagination li a{position:relative;z-index:666;display:block;height:8px;color:#5f6a72;text-align:center;}
.index-pagination li a div{position:relative;z-index:555;display:block;height:8px;background:#FFFFFF;
  -moz-border-radius: 8px; /* Firefox */
  -webkit-border-radius: 8px; /* Safari 和 Chrome */
  border-radius: 8px; /* Opera 10.5+, 以及使用了IE-CSS3的IE浏览器 */
	filter:alpha(opacity=90);
	-moz-opacity:0.9;
	-khtml-opacity: 0.9; 
	opacity: 0.9; 
}
.index-pagination li a div:hover{
	filter:alpha(opacity=100);
	-moz-opacity:1;
	-khtml-opacity: 1; 
	opacity:1; 	
}
.index-pagination li a div span{position:absolute;top:0px;left:0px;height:8px;width:0px;background:#c9242b;
  -moz-border-radius: 8px; /* Firefox */
  -webkit-border-radius: 8px; /* Safari 和 Chrome */
  border-radius: 8px; /* Opera 10.5+, 以及使用了IE-CSS3的IE浏览器 */
}
.index-pagination li a div em{position:absolute;display:none;width:27px;height:12px;left:1px;}
.index-pagination li.current a{color:#0b4da2;}
.index-pagination li.back{display:block;position:absolute;width:27px;height:12px;left:0px;top:4px;z-index:444;}
