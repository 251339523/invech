.lottery_clear{ clear:both; height:0; width:0;}
/*左侧菜单CSS*/
.left{ width:100%; margin:0 auto; font-size:12px;}
.left ul#menu, ul#menu ul {list-style-type:none;margin: 0;padding: 0; width: 100%;}
.left ul#menu a {display: block;text-decoration: none;	}
.left ul#menu li {margin-top: 1px;}
.left ul#menu li a {background: #333;color: #fff;padding: 0.5em;border-left: 5px  #900 solid;}
.left ul#menu li a:hover,#menu li.current{background: #000;}
.left ul#menu li a .hover {background: #000;border-left: 5px  #900 solid;}
.left ul#menu li ul li a {background: #ccc;color: #000;padding-left: 20px;}
.left ul#menu li ul li a:hover {background: #aaa;border-left: 5px #000 solid;padding-left: 15px;}
