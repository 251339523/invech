.content .top{background: #efedea;font-size:12px; color:#7d7d7c; line-height:76px;width: 216px;float: right;height: 76px;border: 1px solid #d28c41;border-radius: 5px;text-align: center;font-size: 16px;}
.content .left{ float:left; width:838px;}
