    function refresh_money() {
        $.ajax({
            cache: false,
            url: "/index/home.html",
            method: 'get',
            data: {ajax: 1, get_money: 1},
            success: function (data) {
                if (data == "") {
                } else {
                    $("#money,#money_2,#user_money").html(data);
                }
            }
        });
        window.setTimeout("refresh_money();", 10000);
    }

    refresh_money();

