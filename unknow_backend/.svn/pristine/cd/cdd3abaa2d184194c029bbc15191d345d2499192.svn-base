.modify_wp{
    width: 979px;
    height: auto;
    margin: 30px auto;
}
.list_table_wp{
    width: 979px;
    border-top:3px solid #2d3036;
    margin-top: 20px;
    background: #ffffff;
    overflow: hidden;
}
.list_table_title{
    width: 100%;
    height: 32px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
}
.list_table_title span{
    display: block;
    float: left;
    width: 19.88%;
    text-align: center;
    line-height: 32px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    background: #f8f8f6;
    font-size: 14px;
}

.list_Tbody_wp{
    width: 100%;
    overflow: hidden;
}
.list_Tbody_wp li{
    float: left;
    width: 100%;
    height: 42px;
    color: #000000;
    border: 1px solid #dbdbdb;
    border-bottom: none;
    border-right:none;
    cursor: pointer;
}
.list_Tbody_wp li.li_add_border_d{
    border-style: dashed;
    border-color: #ff7433;
    border-bottom: none;
}
.list_Tbody_wp li.li_add_border_top{
    border-top-style:dashed;
    border-top-color: #ff7433;
}
.list_Tbody_wp li span{
    display: block;
    width: 19.88%;
    float: left;
    height: 100%;
    text-align: center;
    border-right:1px solid #dbdbdb;
}
.list_Tbody_wp li span i{
    display: block;
    font-style: normal;
    margin-top: 5px;
}
.list_Tbody_wp li span b.list_Tbody_time{
    display: block;
    font-weight: normal;
}
.list_Tbody_wp li span b.list_Tbody_totle{
    display: block;
    font-weight: normal;
    line-height: 42px;
    /*border-right: 1px solid #d2d2d2;*/
}
.list_Tbody_wp li span b.list_Tbody_totle.list_Tbody_totle_right{
    border-right: 1px dashed #ff7433;
}
.list_Tbody_wp li span em{
    line-height: 42px;
    font-style: normal;
}
.list_Tbody_wp li span a{
    line-height: 42px;
    color: #000000;
}
.list_table_tfooter{
    width: 100%;
    height: 44px;
    border-left: 1px solid #dbdbdb;
    border-right: 1px solid #dbdbdb;
    border-bottom: 1px solid #dbdbdb;
    clear: both;
    background: #fffcdf;
}
.list_table_tfooter span{
    display: block;
    float: left;
    width: 19.88%;
    text-align: center;
    line-height: 44px;
    color: #000000;
    border-right: 1px solid #dbdbdb;
    font-size: 14px;
}
.list_table_tfooter span a{
    color: #000000;
}

.table_button_wp{
    width: 99.8%;
    clear: both;
    height: 198px;
    border: 1px solid #dbdbdb;
    border-top:none;
}
.table_button_wp .button_fist{
    margin-left: 395px;
}
.table_button_wp button{
    display: block;
    float: left;
    width: 90px;
    height: 26px;
    text-align: center;
    line-height: 26px;
    background: #f8f8f6;
    border: 1px solid #dbdbdb;
    outline: none;
    border-radius: 30px;
    margin-right: 10px;
    cursor: pointer;
    margin-top: 98px;
}
.table_button_wp button:hover{
    background: #262522;
    color: #ffffff;
}
.table_button_wp span{
    color: #000000;
    line-height: 26px;
    float: left;
    margin-top: 98px;
}

.Lottery_type_wp{
    width: 979px;
    padding: 5px;
    margin-top: 30px;
    background: #ffffff;
    overflow: hidden;
    clear: both;
}
.Lottery_type{
    clear: both;
    width: 65%;
    margin-top: 10px;
    float: left;
}
.Lottery_type li{
    float: left;
    margin-bottom: 10px;
}
.Lottery_type h4{
    float: left;
    text-align: center;
    line-height: 23px;
    color: #ffffff;
    background: #262522;
    border-radius: 30px;
    padding: 2px 16px;
    margin-left: 10px;
}
.Lottery_type a{
    display: inline-block;
    float: left;
    text-align: center;
    line-height: 23px;
    background: #f8f8f6;
    border-radius: 30px;
    padding: 2px 16px;
    margin-left: 10px;
    border: 1px solid #dbdbdb;
    color: #000000;
}
.Lottery_type a.add_background{
    background: #ff4e00;
    color: #ffffff;
}
.lottery_time_wp{
    width: 35%;
    float: right;
    color: #000000;
    height: 100%;
}
.lottery_time{
    margin-top: 8px;
}
.lottery_time span{
    display: block;
    float: left;
    height: 27px;
    line-height: 27px;
    margin-left: 5px;
    margin-right: 5px;
}
.lottery_time button{
    display: block;
    float: left;
    width: 68px;
    height: 27px;
    background: #ff6c00;
    color: #ffffff;
    text-align: center;
    line-height: 27px;
    outline: none;
    border-radius: 8px;
    border: none;
    cursor: pointer;
    margin-left: 5px;
}
.lottery_time button:hover{
    background: #f23d08;
}
.start_time_input{
    width: 82px;
    height: 27px;
    border: 1px solid #a4a4a4;
    border-radius: 8px;
    text-align: center;
    display: block;
    float: left;
    outline: none;
    line-height: 27px;
}
